snippet sec "Comment Section" b
" ===
" === $1
" ===
$2
endsnippet

snippet lua "lua << EOF"
lua << EOF
$1
EOF
endsnippet

snippet no "nnoremap"
noremap <silent> $1 <cr>
endsnippet

snippet sil "silent"
<silent> $1
endsnippet

snippet cmd "<cmd>"
<cmd>$1<cr>
endsnippet

snippet tmpl "vim header template"
" FileName: ${0:`!p snip.rv = (snip.basename or "name") + ".vim"`}
" Description: $1
endsnippet
