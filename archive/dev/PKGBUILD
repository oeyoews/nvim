pkgname=neovim-dev
pkgver=0.9+dev
pkgrel=1
pkgdesc="neovim nightly personaly"
arch=('x86_64')
url="https://neovim.io"
install=${pkgname}.install
license=('custom:neovim')
conflicts=("neovim"
  "nvim")
filename="nvim-linux64.deb"
source=("https://github.com/neovim/neovim/releases/download/nightly/$filename")
md5sums=('SKIP')

package() {
	bsdtar -xf data.tar.gz -C "$pkgdir/"
}
